[ui]
username = monzou <1984tkr@gmail.com>
ignore = ~/.hgignore_global
[extensions]
color =
graphlog =
pager =
fetch =
mq =
shelve =
hgflow = ~/Dropbox/dev/env/hgflow/src/hgflow.py
[flow]
autoshelve = true
[pager]
pager = LESS='FRX' less
quiet = True
ignore = version, help, update
attend = annotate, cat, diff, d, export, glog, gl, log, l, qdiff
[alias]
s = status
diff = d
l = log
ll = log --rev tip:0 --follow
gl = glog
gll = glog --rev tip:0 --follow
sh = shelve
ush = unshelve

%include ~/.hgrc.local
