[ui]
username = monzou <1984tkr@gmail.com>
ignore = ~/.hgignore_global
[extensions]
color =
graphlog =
pager =
fetch =
mq =
shelve =
hgflow = ~/Dropbox/dev/env/hgflow/src/hgflow.py
hgext.collapse = ~/Dropbox/dev/env/hgcollapse/hgext/collapse.py
[flow]
autoshelve = true
[pager]
pager = LESS='FRX' less
quiet = True
ignore = version, help, update
attend = annotate, cat, diff, d, export, glog, gl, log, l, qdiff
[alias]
s = status
diff = d
l = log
ll = log -b .
gl = glog
gll = glog -b .
sh = shelve
ush = unshelve

%include ~/.hgrc.local
